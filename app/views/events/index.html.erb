<p id="notice"><%= notice %></p>

<h1>Tracking</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Event type</th>
      <th>Properties</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.event_type %></td>
        <td><%= event.properties %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>